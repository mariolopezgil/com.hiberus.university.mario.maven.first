pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                bat 'mvn clean test -f pom.xml'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                 withSonarQubeEnv('SonarQube Scanner') {
                                    bat 'mvn sonar:sonar'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}